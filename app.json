{
    "expo": {
        "name": "Spot the same",
        "slug": "spot-the-same",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "scheme": "spotthesame",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "NSCameraUsageDescription": "We use the camera so you can take a profile photo.",
                "NSPhotoLibraryUsageDescription": "We access your library so you can pick a profile photo."
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
        },
        "plugins": ["expo-router", "expo-web-browser", "expo-audio", "expo-asset"]
    }
}
